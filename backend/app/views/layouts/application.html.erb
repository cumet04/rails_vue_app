<!DOCTYPE html>
<html>
  <head>
    <title>RailsVue</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <script type="text/javascript">
      window._application = {
        path: "<%= request.fullpath %>",
        props: JSON.parse('<%= prop_data_json %>')
      }
    </script>
  </head>
  <body>
    <div id="app"></div>
    <script src="<%= assets_url("bundle.js") %>"></script>
  </body>
</html>
